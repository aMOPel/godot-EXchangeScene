; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "SceneSwitcher",
"language": "GDScript",
"path": "res://addons/scene_switcher/scene_switcher.gd"
} ]
_global_script_class_icons={
"SceneSwitcher": ""
}

[application]

config/name="godot-scene-switcher"
run/main_scene="res://example/main.tscn"
config/icon="res://icon.png"

[autoload]

SceneSwitcherManager="*res://addons/scene_switcher/scene_switcher_manager.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/gut/plugin.cfg", "res://addons/scene_switcher/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
